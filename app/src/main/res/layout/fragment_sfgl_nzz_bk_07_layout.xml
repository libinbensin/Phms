<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/dp10" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5.0dip"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="180dip"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="right"
            android:text="脑卒中："
            android:textColor="@color/text_gray"
            android:textSize="20sp" />

        <net.xinhuaxing.widget.RadioGroup
            android:id="@+id/sfyjRadioGroup"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:widgetTag="脑卒中报卡-个人系统疾病史-脑卒中" >

            <RadioButton
                android:id="@+id/sfyj01RadioButton"
                style="@style/radio_button_style" />

            <RadioButton
                android:id="@+id/sfyj02RadioButton"
                style="@style/radio_button_style"
                android:layout_marginLeft="50.0dip" />
        </net.xinhuaxing.widget.RadioGroup>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dip"
            android:gravity="left|center"
            android:orientation="horizontal" >

            <net.xinhuaxing.widget.EditText
                android:layout_width="400.0dip"
                android:layout_height="wrap_content"
                android:background="@drawable/edit_bg"
                android:hint="请详细描述"
                android:numeric="integer"
                app:widgetTag="脑卒中报卡-个人系统疾病史-脑卒中-请详细描述" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5.0dip"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="180dip"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="right"
            android:text="其他："
            android:textColor="@color/text_gray"
            android:textSize="20sp" />

        <net.xinhuaxing.widget.EditText
            android:layout_width="400.0dip"
            android:layout_height="wrap_content"
            android:background="@drawable/edit_bg"
            android:numeric="integer"
            app:widgetTag="脑卒中报卡-个人系统疾病史-其他" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/jktj_title"
            android:gravity="left|center"
            android:orientation="vertical" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="10.0dip"
                android:paddingRight="30.0dip"
                android:text="脑卒中情况"
                android:textColor="@color/jz_text_color"
                android:textSize="@dimen/dp20" />
        </LinearLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1.0dip"
            android:background="@drawable/jktj_line" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp10"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal" >

            <TextView
                style="@style/title_right_text_style"
                android:layout_width="100.0dip"
                android:layout_height="wrap_content"
                android:text="发病时间：" />

            <net.xinhuaxing.widget.CalendarText
                android:id="@+id/csrqCalendarText"
                android:layout_width="180.0dip"
                android:layout_height="wrap_content"
                android:background="@drawable/edit_date_bg"
                android:gravity="left|center_vertical"
                android:textColor="@color/text_gray"
                android:textSize="@dimen/dp20"
                app:widgetTag="脑卒中报卡-个人系统疾病史-脑卒中情况-发病时间" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp30"
            android:gravity="center"
            android:orientation="horizontal"
            android:textColor="@color/text_gray" >

            <TextView
                style="@style/title_right_text_style"
                android:layout_width="100.0dip"
                android:layout_height="wrap_content"
                android:text="住院号：" />

            <net.xinhuaxing.widget.EditText
                android:id="@+id/lxdhEditText"
                style="@style/integer_edittext_style"
                android:layout_width="200.0dip"
                android:layout_height="40.0dip"
                app:widgetTag="脑卒中报卡-个人系统疾病史-脑卒中情况-住院号" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp10"
            android:gravity="center"
            android:orientation="horizontal" >

            <TextView
                style="@style/title_right_text_style"
                android:layout_width="120.0dip"
                android:layout_height="wrap_content"
                android:text="简要病情：" />

            <net.xinhuaxing.widget.EditText
                android:id="@+id/mzSpinnerUtil"
                style="@style/text_edittext_style"
                android:layout_width="200.0dip"
                android:layout_height="40.0dip"
                app:widgetTag="脑卒中报卡-个人系统疾病史-脑卒中情况-简要病情" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5.0dip"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="180dip"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="right"
            android:text="临床诊断："
            android:textColor="@color/text_gray"
            android:textSize="20sp" />

        <net.xinhuaxing.widget.RadioGroup
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:widgetTag="脑卒中报卡-个人系统疾病史-脑卒中情况-临床诊断" >

            <RadioButton
                android:id="@+id/lczd01RadioButton"
                style="@style/radio_button_style" />

            <RadioButton
                android:id="@+id/lczd02RadioButton"
                style="@style/radio_button_style"
                android:layout_marginLeft="50.0dip" />

            <RadioButton
                android:id="@+id/sfyj03RadioButton"
                style="@style/radio_button_style"
                android:layout_marginLeft="50.0dip" />
        </net.xinhuaxing.widget.RadioGroup>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dip"
            android:gravity="left|center"
            android:orientation="horizontal" >

            <net.xinhuaxing.widget.EditText
                android:layout_width="400.0dip"
                android:layout_height="wrap_content"
                android:background="@drawable/edit_bg"
                android:numeric="integer"
                app:widgetTag="脑卒中报卡-个人系统疾病史-脑卒中情况-临床诊断-请详细描述" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingTop="@dimen/dp10" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/jktj_title"
                android:gravity="left|center"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="10.0dip"
                    android:paddingRight="30.0dip"
                    android:text="辅助检查"
                    android:textColor="@color/jz_text_color"
                    android:textSize="@dimen/dp20" />
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="1.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/jktj_line" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/cm_add_text"
                    android:textColor="@color/gray"
                    android:textSize="@dimen/dp20" />

                <ImageView
                    android:id="@+id/xzImageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10.0dip"
                    android:layout_marginRight="20.0dip"
                    android:contentDescription="@string/hello_world"
                    android:src="@drawable/add_label_bg" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="200dip"
            android:layout_marginLeft="@dimen/dp30"
            android:layout_marginRight="@dimen/dp30"
            android:layout_marginTop="@dimen/dp20"
            android:orientation="vertical" >

            <com.cking.phss.widget.ListView
                android:id="@+id/tnbyyqkListView"
                style="@style/listview_style"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                app:addButton="@id/xzImageView"
                app:addDialogLayout="@layout/dialog_add_fzjc_layout2"
                app:evenLineBackgroundColor="@color/list_osh_background_color"
                app:evenLineTextColor="@color/black"
                app:listItemLayout="@layout/item_fzjc_layout2"
                app:oddLineBackgroundColor="@color/list_jsh_background_color"
                app:oddLineTextColor="@color/black"
                app:titleLayout="@layout/item_fzjc_title_layout2"
                app:type="excel"
                app:widgetTag="脑卒中报卡-个人系统疾病史-辅助检查" >
            </com.cking.phss.widget.ListView>
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5.0dip"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left|center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="180.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="血常规：Hb："
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <net.xinhuaxing.widget.EditText
                    android:id="@+id/kfxtzEditText"
                    style="@style/decimal_edittext_style"
                    android:layout_width="60dip"
                    android:layout_height="wrap_content"
                    app:widgetTag="脑卒中报卡-个人系统疾病史-Hb" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50.0dip"
                android:gravity="left|center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="70.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="WBC："
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <net.xinhuaxing.widget.EditText
                    android:id="@+id/thxhdbzEditText"
                    style="@style/decimal_edittext_style"
                    android:layout_width="60dip"
                    android:layout_height="wrap_content"
                    app:widgetTag="脑卒中报卡-个人系统疾病史-WBC" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50.0dip"
                android:gravity="left|center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="70.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="RBC："
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <net.xinhuaxing.widget.EditText
                    style="@style/decimal_edittext_style"
                    android:layout_width="60dip"
                    android:layout_height="wrap_content"
                    app:widgetTag="脑卒中报卡-个人系统疾病史-RBC" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50.0dip"
                android:gravity="left|center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="70.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="CRP："
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <net.xinhuaxing.widget.EditText
                    style="@style/decimal_edittext_style"
                    android:layout_width="60dip"
                    android:layout_height="wrap_content"
                    app:widgetTag="脑卒中报卡-个人系统疾病史-CRP" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50.0dip"
                android:gravity="left|center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="70.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="ESP："
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <net.xinhuaxing.widget.EditText
                    style="@style/decimal_edittext_style"
                    android:layout_width="60dip"
                    android:layout_height="wrap_content"
                    app:widgetTag="脑卒中报卡-个人系统疾病史-ESP" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>