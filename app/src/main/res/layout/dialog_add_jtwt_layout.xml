<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:spinnerutil="http://schemas.android.com/apk/res/com.cking.phss"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:gravity="center"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="700.0dip"
        android:layout_height="wrap_content"
        android:background="@drawable/dialog_bg"
        android:orientation="vertical" >

        <ImageView
            android:id="@+id/closeImageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:src="@drawable/close_bg" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="130.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="阶段："
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <com.cking.phss.widget.SpinnerUtil
                    android:id="@+id/jdSpinnerUtil"
                    spinnerutil:name="jd"
                    android:layout_width="180dip"
                    android:layout_height="40.0dip"
                    android:background="@drawable/spinner_bg" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp20"
                android:layout_marginRight="@dimen/dp30"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="130.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="发生人："
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <EditText
                    android:id="@+id/fsrEditText"
                    android:layout_width="wrap_content"
                    android:layout_height="40.0dip"
                    android:background="@drawable/edit_bg" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp20"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="130.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="发生日期："
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <com.cking.phss.widget.CalendarText
                    android:id="@+id/fsrqCalendarText"
                    android:layout_width="180dip"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edit_date_bg"
                    android:gravity="left|center_vertical"
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/dp30"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="130.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="问题评估："
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <EditText
                    android:id="@+id/wtpgEditText"
                    android:layout_width="wrap_content"
                    android:layout_height="40.0dip"
                    android:background="@drawable/edit_bg" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp20"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="130.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="主要问题："
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <com.cking.phss.widget.MultipleChoiceText
                    android:id="@+id/zywtMultipleChoiceText"
                    spinnerutil:name="zywt"
                    style="@style/spinnerbtndropdown"
                    android:layout_width="490dip"
                    android:layout_height="40.0dip"
                    android:background="@drawable/edit_bg"
                    android:textColor="@color/text_gray" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp20"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="130.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="处理及结果："
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <EditText
                    android:id="@+id/cljjgEditText"
                    android:layout_width="490dip"
                    android:layout_height="40.0dip"
                    android:background="@drawable/edit_bg"
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp20"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="130.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="主观资料："
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <EditText
                    android:id="@+id/zgzlEditText"
                    android:layout_width="180dip"
                    android:layout_height="40.0dip"
                    android:background="@drawable/edit_bg"
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp20"
                android:layout_marginRight="@dimen/dp30"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="130.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="客观资料："
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <EditText
                    android:id="@+id/kgzlEditText"
                    android:layout_width="wrap_content"
                    android:layout_height="40.0dip"
                    android:background="@drawable/edit_bg"
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp20"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="130.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="其他："
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <EditText
                    android:id="@+id/qtEditText"
                    android:layout_width="180dip"
                    android:layout_height="40.0dip"
                    android:background="@drawable/edit_bg"
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp20"
                android:layout_marginRight="@dimen/dp30"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="130.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="管理计划："
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <EditText
                    android:id="@+id/gljhEditText"
                    android:layout_width="wrap_content"
                    android:layout_height="40.0dip"
                    android:background="@drawable/edit_bg"
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp20"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="130.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="记录医生："
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <EditText
                    android:id="@+id/jlysEditText"
                    android:layout_width="180dip"
                    android:layout_height="40.0dip"
                    android:background="@drawable/edit_bg"
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp20"
                android:layout_marginRight="@dimen/dp30"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="130.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="记录日期："
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <com.cking.phss.widget.CalendarText
                    android:id="@+id/jlrqCalendarText"
                    android:layout_width="180dip"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edit_date_bg"
                    android:gravity="left|center_vertical"
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp20"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="130.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="备注："
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <EditText
                    android:id="@+id/bzEditText"
                    android:layout_width="180dip"
                    android:layout_height="40.0dip"
                    android:background="@drawable/edit_bg"
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dp20"
            android:layout_marginTop="@dimen/dp20"
            android:gravity="center"
            android:orientation="horizontal" >

            <Button
                android:id="@+id/qdButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/dp20"
                android:background="@drawable/comfrim_bg"
                android:text="@string/cm_confirm_text"
                android:textColor="@color/confrim_text_color"
                android:textSize="@dimen/dp20" />

            <Button
                android:id="@+id/qxButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp20"
                android:background="@drawable/cancel_bg"
                android:text="@string/cm_cancel_text"
                android:textColor="@color/cancel_text_color"
                android:textSize="@dimen/dp20" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>