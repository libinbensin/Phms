<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:spinnerutil="http://schemas.android.com/apk/res/com.cking.phss"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:gravity="center"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="700.0dip"
        android:layout_height="wrap_content"
        android:background="@drawable/dialog_bg"
        android:orientation="vertical" >

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:src="@drawable/close_bg" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="130.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="@string/jbxx_dia_szxs_text"
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <Spinner
                    android:id="@+id/province_spinner"
                    style="@style/spinnerbtndropdown"
                    android:background="@drawable/spinner_bg"
                    android:prompt="@string/province_prompt" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/dp30"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="130.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="@string/jbxx_dia_s_text"
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <Spinner
                    android:id="@+id/city_spinner"
                    style="@style/spinnerbtndropdown"
                    android:background="@drawable/spinner_bg"
                    android:prompt="@string/city_prompt" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp20"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="130.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="@string/jbxx_dia_qx_text"
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <Spinner
                    android:id="@+id/district_spinner"
                    style="@style/spinnerbtndropdown"
                    android:background="@drawable/spinner_bg"
                    android:prompt="@string/district_prompt" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/dp30"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="130.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="@string/jbxx_dia_jd_text"
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <Spinner
                    android:id="@+id/street_spinner"
                    style="@style/spinnerbtndropdown"
                    android:background="@drawable/spinner_bg"
                    android:prompt="@string/street_prompt" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp20"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="130.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="@string/jbxx_dia_jwsq_text"
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <Spinner
                    android:id="@+id/zone_spinner"
                    style="@style/spinnerbtndropdown"
                    android:background="@drawable/spinner_bg"
                    android:prompt="@string/district_prompt" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/dp30"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="130.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="@string/jbxx_dia_l_text"
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <Spinner
                    android:id="@+id/road_spinner"
                    style="@style/spinnerbtndropdown"
                    android:background="@drawable/spinner_bg"
                    android:prompt="@string/street_prompt" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp20"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="130.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="@string/jbxx_dia_n_text"
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <EditText
                    android:id="@+id/n_edit"
                    android:layout_width="100dip"
                    android:layout_height="36dip"
                    android:background="@drawable/edit_box"
                    android:numeric="integer"
                    android:paddingLeft="10dip"
                    android:singleLine="true"
                    android:textSize="20sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="96.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="@string/jbxx_dia_h_text"
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <EditText
                    android:id="@+id/h_edit"
                    android:layout_width="100dip"
                    android:layout_height="36dip"
                    android:background="@drawable/edit_box"
                    android:numeric="integer"
                    android:paddingLeft="10dip"
                    android:singleLine="true"
                    android:textSize="20sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="96.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="@string/jbxx_dia_s2_text"
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <EditText
                    android:id="@+id/s_edit"
                    android:layout_width="100dip"
                    android:layout_height="36dip"
                    android:background="@drawable/edit_box"
                    android:numeric="integer"
                    android:paddingLeft="10dip"
                    android:singleLine="true"
                    android:textSize="20sp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp20"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="130.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="@string/jbxx_dia_other_text"
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp20" />

                <EditText
                    android:id="@+id/otherEditText"
                    android:layout_width="490dip"
                    android:layout_height="36dip"
                    android:background="@drawable/edit_box"
                    android:paddingLeft="10dip"
                    android:singleLine="true"
                    android:textSize="20sp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dp20"
            android:layout_marginTop="@dimen/dp20"
            android:gravity="center"
            android:orientation="horizontal" >

            <Button
                android:id="@+id/ok_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/dp20"
                android:background="@drawable/comfrim_bg"
                android:text="@string/cm_confirm_text"
                android:textColor="@color/confrim_text_color"
                android:textSize="@dimen/dp20" />

            <Button
                android:id="@+id/cancel_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp20"
                android:background="@drawable/cancel_bg"
                android:text="@string/cm_cancel_text"
                android:textColor="@color/cancel_text_color"
                android:textSize="@dimen/dp20" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>