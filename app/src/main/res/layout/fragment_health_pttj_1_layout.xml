                    android:layout_alignLeft="@+id/zz09CheckBox"
                    android:layout_below="@+id/zz09CheckBox"
                    android:layout_marginTop="20dip"
                    android:text="@string/pttj_slmh_text" />

                <CheckBox
                    android:id="@+id/zz16CheckBox"
                    style="@style/compound_button_style"
                    android:layout_alignLeft="@+id/zz10CheckBox"
                    android:layout_below="@+id/zz10CheckBox"
                    android:layout_marginTop="20dip"
                    android:text="@string/pttj_sjmm_text" />

                <CheckBox
                    android:id="@+id/zz17CheckBox"
                    style="@style/compound_button_style"
                    android:layout_alignLeft="@+id/zz11CheckBox"
                    android:layout_below="@+id/zz11CheckBox"
                    android:layout_marginTop="20dip"
                    android:text="@string/pttj_nj_text" />

                <CheckBox
                    android:id="@+id/zz18CheckBox"
                    style="@style/compound_button_style"
                    android:layout_alignLeft="@+id/zz12CheckBox"
                    android:layout_below="@+id/zz12CheckBox"
                    android:layout_marginTop="20dip"
                    android:text="@string/pttj_ntong_text" />

                <CheckBox
                    android:id="@+id/zz19CheckBox"
                    style="@style/compound_button_style"
                    android:layout_alignLeft="@+id/zz13CheckBox"
                    android:layout_below="@+id/zz13CheckBox"
                    android:layout_marginTop="20dip"
                    android:text="@string/pttj_bm_text" />

                <CheckBox
                    android:id="@+id/zz20CheckBox"
                    style="@style/compound_button_style"
                    android:layout_alignLeft="@+id/zz14CheckBox"
                    android:layout_below="@+id/zz14CheckBox"
                    android:layout_marginTop="20dip"
                    android:text="@string/pttj_fx_text" />

                <CheckBox
                    android:id="@+id/zz21CheckBox"
                    style="@style/compound_button_style"
                    android:layout_alignLeft="@+id/zz15CheckBox"
                    android:layout_below="@+id/zz15CheckBox"
                    android:layout_marginTop="20dip"
                    android:text="@string/pttj_exot_text" />

                <CheckBox
                    android:id="@+id/zz22CheckBox"
                    style="@style/compound_button_style"
