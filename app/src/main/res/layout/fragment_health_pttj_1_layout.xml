                <EditText
                    android:id="@+id/qtEditText"
                    android:layout_width="170.0dip"
                    android:layout_height="40.0dip"
                    android:layout_alignBaseline="@+id/zz25CheckBox"
                    android:layout_marginLeft="@dimen/dp10"
                    android:layout_toRightOf="@+id/zz25CheckBox"
                    android:background="@drawable/edit_bg"
                    android:hint="@string/personal_qt_text"
                    android:textSize="@dimen/dp20" />
            </com.adinnet.xd.medical.widget.RelativeGroup>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>