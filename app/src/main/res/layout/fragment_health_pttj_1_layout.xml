                    android:layout_marginTop="20dip"
                    android:text="@string/pttj_yh_text" />

                <CheckBox
                    android:id="@+id/zz23CheckBox"
                    style="@style/compound_button_style"
                    android:layout_alignLeft="@+id/zz17CheckBox"
                    android:layout_below="@+id/zz17CheckBox"
                    android:layout_marginTop="20dip"
                    android:text="@string/pttj_em_text" />

                <CheckBox
                    android:id="@+id/zz24CheckBox"
                    style="@style/compound_button_style"
                    android:layout_alignLeft="@+id/zz18CheckBox"
                    android:layout_below="@+id/zz18CheckBox"
                    android:layout_marginTop="20dip"
                    android:text="@string/pttj_rfzt_text" />

                <CheckBox
                    android:id="@+id/zz25CheckBox"
