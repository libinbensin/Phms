<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/dp10" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/jktj_title"
            android:gravity="left|center"
            android:orientation="vertical" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="10.0dip"
                android:paddingRight="30.0dip"
                android:text="基本信息"
                android:textColor="@color/jz_text_color"
                android:textSize="@dimen/dp20" />
        </LinearLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1.0dip"
            android:background="@drawable/jktj_line" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp30"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                style="@style/title_right_text_style"
                android:layout_width="100.0dip"
                android:layout_height="wrap_content"
                android:text="身份证：" />

            <net.xinhuaxing.widget.EditText
                android:id="@+id/sfzEditText"
                style="@style/idc_edittext_style"
                android:layout_width="wrap_content"
                android:layout_height="40.0dip"
                app:widgetTag="脑卒中报卡-基本信息-身份证" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp10"
            android:orientation="horizontal" >

            <TextView
                style="@style/title_right_text_style"
                android:layout_width="100.0dip"
                android:layout_height="wrap_content"
                android:text="姓名：" />

            <net.xinhuaxing.widget.EditText
                android:id="@+id/xmEditText"
                style="@style/text_edittext_style"
                android:layout_width="wrap_content"
                android:layout_height="40.0dip"
                app:widgetTag="脑卒中报卡-基本信息-姓名" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp30"
            android:gravity="center"
            android:orientation="horizontal" >

            <TextView
                style="@style/title_right_text_style"
                android:layout_width="100.0dip"
                android:layout_height="wrap_content"
                android:text="性别：" />

            <net.xinhuaxing.widget.Spinner
                android:id="@+id/xbSpinnerUtil"
                style="@style/spinnerbtndropdown"
                android:background="@drawable/spinner_bg"
                android:prompt="@string/gender_prompt"
                app:widgetTag="脑卒中报卡-基本信息-性别" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp10"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal" >

            <TextView
                style="@style/title_right_text_style"
                android:layout_width="100.0dip"
                android:layout_height="wrap_content"
                android:text="出生日期：" />

            <net.xinhuaxing.widget.CalendarText
                android:id="@+id/csrqCalendarText"
                android:layout_width="180.0dip"
                android:layout_height="wrap_content"
                android:background="@drawable/edit_date_bg"
                android:gravity="left|center_vertical"
                android:textColor="@color/text_gray"
                android:textSize="@dimen/dp20"
                app:widgetTag="脑卒中报卡-基本信息-出生日期" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp30"
            android:gravity="center"
            android:orientation="horizontal"
            android:textColor="@color/text_gray" >

            <TextView
                style="@style/title_right_text_style"
                android:layout_width="100.0dip"
                android:layout_height="wrap_content"
                android:text="本人电话：" />

            <net.xinhuaxing.widget.EditText
                android:id="@+id/lxdhEditText"
                style="@style/phone_edittext_style"
                android:layout_width="200.0dip"
                android:layout_height="40.0dip"
                app:widgetTag="脑卒中报卡-基本信息-本人电话" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp10"
            android:gravity="center"
            android:orientation="horizontal" >

            <TextView
                style="@style/title_right_text_style"
                android:layout_width="80.0dip"
                android:layout_height="wrap_content"
                android:text="职业：" />

            <net.xinhuaxing.widget.Spinner
                android:id="@+id/mzSpinnerUtil"
                style="@style/spinnerbtndropdown"
                android:background="@drawable/spinner_bg"
                android:prompt="@string/gender_prompt"
                app:widgetTag="脑卒中报卡-基本信息-职业" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp10"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                style="@style/title_right_text_style"
                android:layout_width="100.0dip"
                android:layout_height="wrap_content"
                android:text="联系人：" />

            <net.xinhuaxing.widget.EditText
                style="@style/idc_edittext_style"
                android:layout_width="wrap_content"
                android:layout_height="40.0dip"
                app:widgetTag="脑卒中报卡-基本信息-联系人" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp10"
            android:orientation="horizontal" >

            <TextView
                style="@style/title_right_text_style"
                android:layout_width="120.0dip"
                android:layout_height="wrap_content"
                android:text="联系人关系：" />

            <net.xinhuaxing.widget.Spinner
                style="@style/spinnerbtndropdown"
                android:layout_width="160.0dip"
                android:background="@drawable/spinner_bg"
                android:prompt="@string/gender_prompt"
                app:widgetTag="脑卒中报卡-基本信息-联系人关系" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp30"
            android:gravity="center"
            android:orientation="horizontal" >

            <TextView
                style="@style/title_right_text_style"
                android:layout_width="100.0dip"
                android:layout_height="wrap_content"
                android:text="联系电话：" />

            <net.xinhuaxing.widget.EditText
                style="@style/text_edittext_style"
                android:layout_width="wrap_content"
                android:layout_height="40.0dip"
                app:widgetTag="脑卒中报卡-基本信息-联系电话" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp10"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal" >

            <TextView
                style="@style/title_right_text_style"
                android:layout_width="100.0dip"
                android:layout_height="wrap_content"
                android:text="文化程度：" />

            <net.xinhuaxing.widget.Spinner
                style="@style/spinnerbtndropdown"
                android:background="@drawable/spinner_bg"
                android:prompt="@string/gender_prompt"
                app:widgetTag="脑卒中报卡-基本信息-文化程度" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30.0dip"
            android:gravity="center"
            android:orientation="horizontal" >

            <TextView
                style="@style/title_right_text_style"
                android:layout_width="100.0dip"
                android:layout_height="wrap_content"
                android:text="教育年限：" />

            <net.xinhuaxing.widget.EditText
                style="@style/text_edittext_style"
                android:layout_width="wrap_content"
                android:layout_height="40.0dip"
                app:widgetTag="脑卒中报卡-基本信息-教育年限" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp10"
            android:gravity="center"
            android:orientation="horizontal" >

            <TextView
                style="@style/title_right_text_style"
                android:layout_width="100.0dip"
                android:layout_height="wrap_content"
                android:text="邮政编码：" />

            <net.xinhuaxing.widget.EditText
                style="@style/text_edittext_style"
                android:layout_width="wrap_content"
                android:layout_height="40.0dip"
                app:widgetTag="脑卒中报卡-基本信息-邮政编码" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp10"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                style="@style/title_right_text_style"
                android:layout_width="100.0dip"
                android:layout_height="wrap_content"
                android:text="家庭住址：" />

            <com.cking.phss.widget.AddressText
                android:id="@+id/jtzzAddressText"
                style="@style/text_edittext_style"
                android:layout_width="480.0dip"
                android:layout_height="40.0dip" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp20"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="150.0dip"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:text="经济状况："
            android:textColor="@color/text_gray"
            android:textSize="@dimen/dp20" />

        <net.xinhuaxing.widget.RadioGroup
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:widgetTag="脑卒中报卡-基本信息-经济状况" >

            <RadioButton
                android:id="@+id/nrjsr01Radiobutton"
                style="@style/radio_button_style"
                android:text="&lt;1000元" />

            <RadioButton
                android:id="@+id/nrjsr02Radiobutton"
                style="@style/radio_button_style"
                android:layout_marginLeft="75.0dip"
                android:layout_toRightOf="@id/nrjsr01Radiobutton"
                android:text="1000-3000元" />

            <RadioButton
                android:id="@+id/nrjsr03Radiobutton"
                style="@style/radio_button_style"
                android:layout_marginLeft="75.0dip"
                android:layout_toRightOf="@id/nrjsr02Radiobutton"
                android:text="3000-5000元" />

            <RadioButton
                android:id="@+id/nrjsr04Radiobutton"
                style="@style/radio_button_style"
                android:layout_marginLeft="75.0dip"
                android:layout_toRightOf="@id/nrjsr03Radiobutton"
                android:text=">5000元" />
        </net.xinhuaxing.widget.RadioGroup>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp10"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="100.0dip"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp5"
            android:gravity="right"
            android:text="医疗服务："
            android:textColor="@color/text_gray"
            android:textSize="@dimen/dp20" />

        <net.xinhuaxing.widget.CheckBoxGroup
            android:id="@+id/hxsj_group"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:widgetTag="脑卒中报卡-基本信息-医疗服务" >

            <CheckBox
                android:id="@+id/bxlx01CheckBox"
                style="@style/compound_button_style"
                android:text="@string/sfgl_gxy_bk_bxlx_yb_text" />

            <CheckBox
                android:id="@+id/bxlx02CheckBox"
                style="@style/compound_button_style"
                android:layout_marginLeft="130dip"
                android:layout_toRightOf="@+id/bxlx01CheckBox"
                android:text="@string/sfgl_gxy_bk_bxlx_czzgjbylbx_text" />

            <CheckBox
                android:id="@+id/bxlx03CheckBox"
                style="@style/compound_button_style"
                android:layout_marginLeft="20dip"
                android:layout_toRightOf="@+id/bxlx02CheckBox"
                android:text="@string/sfgl_gxy_bk_bxlx_czjmjbylbx_text" />

            <CheckBox
                android:id="@+id/bxlx04CheckBox"
                style="@style/compound_button_style"
                android:layout_alignLeft="@+id/bxlx01CheckBox"
                android:layout_below="@+id/bxlx01CheckBox"
                android:layout_marginTop="@dimen/dp20"
                android:text="@string/sfgl_gxy_bk_bxlx_xxnchzyl_text" />

            <CheckBox
                android:id="@+id/bxlx05CheckBox"
                style="@style/compound_button_style"
                android:layout_alignLeft="@+id/bxlx02CheckBox"
                android:layout_below="@+id/bxlx02CheckBox"
                android:layout_marginTop="@dimen/dp20"
                android:text="@string/sfgl_gxy_bk_bxlx_pkjz_text" />

            <CheckBox
                android:id="@+id/bxlx06CheckBox"
                style="@style/compound_button_style"
                android:layout_alignLeft="@+id/bxlx03CheckBox"
                android:layout_below="@+id/bxlx03CheckBox"
                android:layout_marginTop="@dimen/dp20"
                android:text="@string/sfgl_gxy_bk_bxlx_syylbx_text" />

            <CheckBox
                android:id="@+id/bxlx07CheckBox"
                style="@style/compound_button_style"
                android:layout_alignLeft="@+id/bxlx04CheckBox"
                android:layout_below="@+id/bxlx04CheckBox"
                android:layout_marginTop="18dip"
                android:text="@string/sfgl_gxy_bk_bxlx_gfyl_text" />

            <CheckBox
                android:id="@+id/bxlx08CheckBox"
                style="@style/compound_button_style"
                android:layout_alignLeft="@+id/bxlx05CheckBox"
                android:layout_below="@+id/bxlx05CheckBox"
                android:layout_marginTop="18dip"
                android:text="@string/sfgl_gxy_bk_bxlx_qzf_text" />

            <CheckBox
                android:id="@+id/bxlx09CheckBox"
                style="@style/compound_button_style"
                android:layout_alignLeft="@+id/bxlx06CheckBox"
                android:layout_below="@+id/bxlx06CheckBox"
                android:layout_marginTop="18dip"
                android:text="@string/sfgl_gxy_bk_bxlx_dbtc_text" />

            <CheckBox
                android:id="@+id/bxlx10CheckBox"
                style="@style/compound_button_style"
                android:layout_alignLeft="@+id/bxlx07CheckBox"
                android:layout_below="@+id/bxlx07CheckBox"
                android:layout_marginTop="18dip"
                android:text="@string/sfgl_gxy_bk_bxlx_qt_text" />
        </net.xinhuaxing.widget.CheckBoxGroup>
    </LinearLayout>

</LinearLayout>