<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:spinnerutil="http://schemas.android.com/apk/res/com.cking.phss"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:paddingTop="@dimen/dp20" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingTop="@dimen/dp10" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/jktj_title"
                android:gravity="left|center"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="10.0dip"
                    android:paddingRight="30.0dip"
                    android:text="个人信息"
                    android:textColor="@color/jz_text_color"
                    android:textSize="@dimen/dp20" />
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="1.0dip"
                android:background="@drawable/jktj_line" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10.0dip"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal" >
            </LinearLayout>

            <TextView
                android:layout_width="130dip"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="right"
                android:text="随访日期:   "
                android:textColor="@color/text_gray"
                android:textSize="@dimen/dp20" />

            <com.cking.phss.widget.CalendarText
                android:id="@+id/sfrqCalendarText"
                android:layout_width="180dip"
                android:layout_height="wrap_content"
                android:background="@drawable/edit_date_bg"
                android:gravity="left|center_vertical"
                android:textColor="@color/text_gray"
                android:textSize="20sp" />

            <TextView
                android:layout_width="130dip"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="right"
                android:text="随访方式:   "
                android:textColor="@color/text_gray"
                android:textSize="@dimen/dp20" />

            <com.cking.phss.widget.SpinnerUtil
                android:id="@+id/sffsSpinnerUtil"
                spinnerutil:name="sffs"
                style="@style/spinnerbtndropdown"
                android:background="@drawable/spinner_bg"
                android:prompt="@string/suifang_prompt" />

            <TextView
                android:layout_width="120dip"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="60.0dip"
                android:gravity="right"
                android:text="困难群体：   "
                android:textColor="@color/text_gray"
                android:textSize="20sp" />

            <com.cking.phss.widget.SpinnerUtil
                android:id="@+id/knqtSpinnerUtil"
                spinnerutil:name="knqt"
                style="@style/spinnerbtndropdown"
                android:background="@drawable/spinner_bg"
                android:prompt="@string/knqt_prompt" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="100.0dip"
                android:gravity="center"
                android:orientation="horizontal" >
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dip"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="130dip"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="康复需求:   "
                android:textColor="@color/text_gray"
                android:textSize="20sp" />

            <com.cking.phss.widget.SpinnerUtil
                android:id="@+id/kfxqSpinnerUtil"
                spinnerutil:name="kfxq"
                style="@style/spinnerbtndropdown"
                android:background="@drawable/spinner_bg"
                android:prompt="@string/kfxq_prompt" />

            <TextView
                android:layout_width="130dip"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="@string/pttj_zrys_text"
                android:textColor="@color/text_gray"
                android:textSize="@dimen/dp20" />

            <EditText
                android:id="@+id/zrysEditText"
                android:layout_width="180dip"
                android:layout_height="wrap_content"
                android:background="@drawable/edit_bg"
                android:ems="10" >
            </EditText>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal"
        android:paddingTop="@dimen/dp20" >

        <LinearLayout
            android:layout_width="140.0dip"
            android:layout_height="wrap_content"
            android:background="@drawable/jktj_title"
            android:gravity="left|center"
            android:orientation="vertical" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="10.0dip"
                android:text="残疾类型"
                android:textColor="@color/jz_text_color"
                android:textSize="@dimen/dp20" />
        </LinearLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1.0dip"
            android:background="@drawable/jktj_line" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10.0dip"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="130dip"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="残疾证编号："
                android:textColor="@color/text_gray"
                android:textSize="@dimen/dp20" />

            <EditText
                android:id="@+id/cjzbhEditText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/edit_bg"
                android:numeric="integer" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="100.0dip"
            android:gravity="center"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="130dip"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="主要残疾："
                android:textColor="@color/text_gray"
                android:textSize="@dimen/dp20" />

            <com.cking.phss.widget.SpinnerUtil
                android:id="@+id/zycjSpinnerUtil"
                spinnerutil:name="zycj"
                style="@style/spinnerbtndropdown"
                android:background="@drawable/spinner_bg"
                android:prompt="@string/zhuyaocanji_prompt" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <com.adinnet.xd.medical.widget.RelativeGroup
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="12dip"
            android:layout_marginTop="18dip" >

            <TextView
                android:id="@+id/bqgshqkTextView"
                android:layout_width="100dip"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dip"
                android:gravity="right"
                android:text="多重残疾："
                android:textColor="@color/text_gray"
                android:textSize="@dimen/dp20" />

            <CheckBox
                android:id="@+id/dccj01CheckBox"
                style="@style/compound_jzs_button_style"
                android:layout_toRightOf="@+id/bqgshqkTextView"
                android:checked="true"
                android:text="@string/sfgl_cjsf_dccj_f_text" />

            <CheckBox
                android:id="@+id/dccj02CheckBox"
                style="@style/compound_jzs_button_style"
                android:layout_marginLeft="@dimen/dp80"
                android:layout_toRightOf="@+id/dccj01CheckBox"
                android:text="@string/sfgl_cjsf_dccj_slcj_text" />

            <CheckBox
                android:id="@+id/dccj03CheckBox"
                style="@style/compound_jzs_button_style"
                android:layout_marginLeft="@dimen/dp80"
                android:layout_toRightOf="@+id/dccj02CheckBox"
                android:text="@string/sfgl_cjsf_dccj_tlcj_text" />

            <CheckBox
                android:id="@+id/dccj04CheckBox"
                style="@style/compound_jzs_button_style"
                android:layout_marginLeft="60.0dip"
                android:layout_toRightOf="@+id/dccj03CheckBox"
                android:text="@string/sfgl_cjsf_dccj_yycj_text" />

            <CheckBox
                android:id="@+id/dccj05CheckBox"
                style="@style/compound_jzs_button_style"
                android:layout_marginLeft="60.0dip"
                android:layout_toRightOf="@+id/dccj04CheckBox"
                android:text="@string/sfgl_cjsf_dccj_ztcj_text" />

            <CheckBox
                android:id="@+id/dccj06CheckBox"
                style="@style/compound_jzs_button_style"
                android:layout_alignLeft="@+id/dccj01CheckBox"
                android:layout_below="@+id/dccj01CheckBox"
                android:layout_marginTop="18dip"
                android:text="@string/sfgl_cjsf_dccj_zlcj_text" />

            <CheckBox
                android:id="@+id/dccj07CheckBox"
                style="@style/compound_jzs_button_style"
                android:layout_alignLeft="@+id/dccj02CheckBox"
                android:layout_below="@+id/dccj01CheckBox"
                android:layout_marginTop="18dip"
                android:text="@string/sfgl_cjsf_dccj_jscj_text" />
        </com.adinnet.xd.medical.widget.RelativeGroup>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dip"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="130dip"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:text="残疾日期："
            android:textColor="@color/text_gray"
            android:textSize="@dimen/dp20" />

        <com.cking.phss.widget.CalendarText
            android:id="@+id/zcrqCalendarText"
            android:layout_width="180dip"
            android:layout_height="wrap_content"
            android:background="@drawable/edit_date_bg"
            android:gravity="left|center_vertical"
            android:textColor="@color/text_gray"
            android:textSize="20sp" />

        <TextView
            android:layout_width="100dip"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10.0dip"
            android:gravity="right"
            android:text="残疾程度："
            android:textColor="@color/text_gray"
            android:textSize="20sp" />

        <com.cking.phss.widget.SpinnerUtil
            android:id="@+id/cjcdSpinnerUtil"
            spinnerutil:name="cjcd"
            style="@style/spinnerbtndropdown"
            android:background="@drawable/spinner_bg"
            android:prompt="@string/cjcd_prompt" />

        <TextView
            android:layout_width="100dip"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10.0dip"
            android:gravity="right"
            android:text="残疾原因："
            android:textColor="@color/text_gray"
            android:textSize="20sp" />

        <com.cking.phss.widget.SpinnerUtil
            android:id="@+id/cjyySpinnerUtil"
            spinnerutil:name="zycj2"
            style="@style/spinnerbtndropdown"
            android:background="@drawable/spinner_bg"
            android:prompt="@string/cjyy_prompt" />
    </LinearLayout>

</LinearLayout>