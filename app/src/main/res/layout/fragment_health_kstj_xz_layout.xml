                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="@string/jktj_dgc_text"
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp18" />

                <EditText
                    android:id="@+id/dgcEditText"
                    android:layout_width="80.0dip"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edit_bg"
                    android:numeric="decimal"
                    android:textSize="@dimen/dp20" />

                <TextView
                    android:id="@+id/dgcdwTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10.0dip"
                    android:text="@string/cm_mmol_l_text"
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp18" />

                <TextView
                    android:layout_width="90.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="@string/jktj_conclusion_text"
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp18" />

                <EditText
                    android:id="@+id/dgcjlEditText"
                    android:layout_width="220.0dip"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edit_trans_bg"
                    android:enabled="false"
                    android:textSize="@dimen/dp20" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10.0dip"
