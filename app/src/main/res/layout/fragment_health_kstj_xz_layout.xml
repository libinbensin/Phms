                    android:layout_width="130.0dip"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="@string/jktj_gmddbz_text"
                    android:textColor="@color/text_gray"
                    android:textSize="@dimen/dp18" />

                <EditText
