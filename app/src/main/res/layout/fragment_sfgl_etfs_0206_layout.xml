<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:paddingTop="@dimen/dp20" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/jktj_title"
            android:gravity="left|center"
            android:orientation="vertical" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="10.0dip"
                android:paddingRight="30.0dip"
                android:text="其他实验室(辅助)检查"
                android:textColor="@color/jz_text_color"
                android:textSize="@dimen/dp20" />
        </LinearLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1.0dip"
            android:layout_weight="1.0"
            android:background="@drawable/jktj_line" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/cm_add_text"
                android:textColor="@color/gray"
                android:textSize="@dimen/dp20" />

            <ImageView
                android:id="@+id/xzImageView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10.0dip"
                android:layout_marginRight="20.0dip"
                android:contentDescription="@string/hello_world"
                android:src="@drawable/add_label_bg" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="200dip"
        android:layout_marginLeft="@dimen/dp30"
        android:layout_marginRight="@dimen/dp30"
        android:layout_marginTop="@dimen/dp20"
        android:orientation="vertical" >

        <com.cking.phss.widget.ListView
            android:id="@+id/tnbyyqkListView"
            style="@style/listview_style"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            app:addButton="@id/xzImageView"
            app:addDialogLayout="@layout/dialog_add_fzjc_layout2"
            app:evenLineBackgroundColor="@color/list_osh_background_color"
            app:evenLineTextColor="@color/black"
            app:listItemLayout="@layout/item_fzjc_layout2"
            app:oddLineBackgroundColor="@color/list_jsh_background_color"
            app:oddLineTextColor="@color/black"
            app:titleLayout="@layout/item_fzjc_title_layout2"
            app:type="excel"
            app:widgetTag="儿童访视-1岁内-其他实验室(辅助)检查" >
        </com.cking.phss.widget.ListView>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal"
        android:paddingTop="@dimen/dp20" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/jktj_title"
            android:gravity="left|center"
            android:orientation="vertical" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="10.0dip"
                android:paddingRight="30.0dip"
                android:text="发育筛选"
                android:textColor="@color/jz_text_color"
                android:textSize="@dimen/dp20" />
        </LinearLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1.0dip"
            android:background="@drawable/jktj_line" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5.0dip"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5.0dip"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="180dip"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="right"
                android:text="俯卧抬头："
                android:textColor="@color/text_gray"
                android:textSize="20sp" />

            <net.xinhuaxing.widget.RadioGroup
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="35.0dip"
                android:orientation="horizontal"
                app:widgetTag="儿童访视-1岁内-发育筛选-俯卧抬头" >

                <RadioButton
                    android:id="@+id/fwtt01RadioButton"
                    style="@style/radio_button_style" />

                <RadioButton
                    android:id="@+id/fwtt02RadioButton"
                    style="@style/radio_button_style"
                    android:layout_marginLeft="50.0dip" />
            </net.xinhuaxing.widget.RadioGroup>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5.0dip"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="200dip"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="right"
                android:text="逗引时会微笑："
                android:textColor="@color/text_gray"
                android:textSize="20sp" />

            <net.xinhuaxing.widget.RadioGroup
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="35.0dip"
                android:orientation="horizontal"
                app:widgetTag="儿童访视-1岁内-发育筛选-逗引时会微笑" >

                <RadioButton
                    android:id="@+id/dyshwx01RadioButton"
                    style="@style/radio_button_style" />

                <RadioButton
                    android:id="@+id/dyshwx02RadioButton"
                    style="@style/radio_button_style"
                    android:layout_marginLeft="50.0dip" />
            </net.xinhuaxing.widget.RadioGroup>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5.0dip"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="200dip"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="right"
                android:text="抱紧时能自己控制头："
                android:textColor="@color/text_gray"
                android:textSize="20sp" />

            <net.xinhuaxing.widget.RadioGroup
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="35.0dip"
                android:orientation="horizontal"
                app:widgetTag="儿童访视-1岁内-发育筛选-抱紧时能自己控制头" >

                <RadioButton
                    android:id="@+id/bjsnzjkzt01RadioButton"
                    style="@style/radio_button_style" />

                <RadioButton
                    android:id="@+id/bjsnzjkzt02RadioButton"
                    style="@style/radio_button_style"
                    android:layout_marginLeft="50.0dip" />
            </net.xinhuaxing.widget.RadioGroup>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5.0dip"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5.0dip"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="180dip"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="right"
                android:text="手张开："
                android:textColor="@color/text_gray"
                android:textSize="20sp" />

            <net.xinhuaxing.widget.RadioGroup
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="35.0dip"
                android:orientation="horizontal"
                app:widgetTag="儿童访视-1岁内-发育筛选-手张开" >

                <RadioButton
                    android:id="@+id/szk01RadioButton"
                    style="@style/radio_button_style" />

                <RadioButton
                    android:id="@+id/szk02RadioButton"
                    style="@style/radio_button_style"
                    android:layout_marginLeft="50.0dip" />
            </net.xinhuaxing.widget.RadioGroup>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5.0dip"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="200dip"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="right"
                android:text="笑出声："
                android:textColor="@color/text_gray"
                android:textSize="20sp" />

            <net.xinhuaxing.widget.RadioGroup
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="35.0dip"
                android:orientation="horizontal"
                app:widgetTag="儿童访视-1岁内-发育筛选-笑出声" >

                <RadioButton
                    android:id="@+id/xcs01RadioButton"
                    style="@style/radio_button_style" />

                <RadioButton
                    android:id="@+id/xcs02RadioButton"
                    style="@style/radio_button_style"
                    android:layout_marginLeft="50.0dip" />
            </net.xinhuaxing.widget.RadioGroup>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5.0dip"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="200dip"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="right"
                android:text="伸手抓玩具："
                android:textColor="@color/text_gray"
                android:textSize="20sp" />

            <net.xinhuaxing.widget.RadioGroup
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="35.0dip"
                android:orientation="horizontal"
                app:widgetTag="儿童访视-1岁内-发育筛选-伸手抓玩具" >

                <RadioButton
                    android:id="@+id/sszwj01RadioButton"
                    style="@style/radio_button_style" />

                <RadioButton
                    android:id="@+id/sszwj02RadioButton"
                    style="@style/radio_button_style"
                    android:layout_marginLeft="50.0dip" />
            </net.xinhuaxing.widget.RadioGroup>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5.0dip"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5.0dip"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="180dip"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="right"
                android:text="会翻身："
                android:textColor="@color/text_gray"
                android:textSize="20sp" />

            <net.xinhuaxing.widget.RadioGroup
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="35.0dip"
                android:orientation="horizontal"
                app:widgetTag="儿童访视-1岁内-发育筛选-会翻身" >

                <RadioButton
                    android:id="@+id/hfs01RadioButton"
                    style="@style/radio_button_style" />

                <RadioButton
                    android:id="@+id/hfs02RadioButton"
                    style="@style/radio_button_style"
                    android:layout_marginLeft="50.0dip" />
            </net.xinhuaxing.widget.RadioGroup>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5.0dip"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="200dip"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="right"
                android:text="照镜子时对自己微笑："
                android:textColor="@color/text_gray"
                android:textSize="20sp" />

            <net.xinhuaxing.widget.RadioGroup
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="35.0dip"
                android:orientation="horizontal"
                app:widgetTag="儿童访视-1岁内-发育筛选-照镜子时对自己微笑" >

                <RadioButton
                    android:id="@+id/zjzsdzjx01RadioButton"
                    style="@style/radio_button_style" />

                <RadioButton
                    android:id="@+id/zjzsdzjx02RadioButton"
                    style="@style/radio_button_style"
                    android:layout_marginLeft="50.0dip" />
            </net.xinhuaxing.widget.RadioGroup>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5.0dip"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="200dip"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="right"
                android:text="认生："
                android:textColor="@color/text_gray"
                android:textSize="20sp" />

            <net.xinhuaxing.widget.RadioGroup
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="35.0dip"
                android:orientation="horizontal"
                app:widgetTag="儿童访视-1岁内-发育筛选-认生" >

                <RadioButton
                    android:id="@+id/rs01RadioButton"
                    style="@style/radio_button_style" />

                <RadioButton
                    android:id="@+id/rs02RadioButton"
                    style="@style/radio_button_style"
                    android:layout_marginLeft="50.0dip" />
            </net.xinhuaxing.widget.RadioGroup>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>